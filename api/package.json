{
  "name": "tinyhoods-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn run clean && nodemon --ignore './dist/' --exec concurrently -n \"TRANSPILE,API\" -c \"blue,magenta\" \"yarn run build:dev\" \"yarn run start:dev\"",
    "queue": "node  dist/server/services/queue/index.js",
    "build": "tsc",
    "build:dev": "yarn run clean && tsc",
    "clean": "rm -rf ./dist/index.js",
    "start:dev": "wait-on ./dist/index.js && micro -l tcp://0.0.0.0:4000 ./dist/index.js",
    "start": "micro -l tcp://0.0.0.0:4000 ./dist/index.js",
    "seeder:build": "tsc -p packages/seeder",
    "seeder:run:landforms": "node dist/packages/seeder/landforms.seed.js",
    "seeder:run:keywords": "node dist/packages/seeder/keywords.seed.js"
  },
  "dependencies": {
    "@stripe/stripe-js": "^1.8.0",
    "@types/ejs": "^3.0.5",
    "@types/mongoose-paginate-v2": "^1.3.9",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-mailgun-transport": "^1.4.2",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-server-micro": "^2.15.1",
    "apollo-utilities": "^1.3.4",
    "bull": "^3.20.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "fs-extra": "^9.0.1",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "microrouter": "^3.1.3",
    "moment": "^2.27.0",
    "mongoose": "^5.9.20",
    "mongoose-paginate-v2": "^1.3.11",
    "nodemailer": "^6.4.17",
    "nodemailer-mailgun-transport": "^2.0.2",
    "ramda": "^0.27.1",
    "request": "^2.88.2",
    "squareicon": "^1.0.3",
    "stripe": "^8.81.0",
    "validator": "^13.1.1",
    "yup": "^0.29.2"
  },
  "devDependencies": {
    "@types/bull": "^3.14.4",
    "@types/fs-extra": "^9.0.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/micro": "^7.3.3",
    "@types/micro-cors": "^0.1.1",
    "@types/microrouter": "^3.1.1",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.7.29",
    "@types/node": "^14.0.14",
    "@types/ramda": "^0.27.14",
    "@types/request": "^2.48.5",
    "@types/stripe": "^7.13.24",
    "@types/validator": "^13.1.0",
    "@types/yup": "^0.29.3",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.5",
    "wait-on": "^5.2.1"
  }
}
